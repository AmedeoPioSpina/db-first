Nome tabella: auto

id - integer - MEDIUMINT - PRIMARY_KEY - UNSIGNED - AUTO_INCREMENT UNIQUE NOT NULL
modello - string - VARCHAR(50) - NOT NULL
marca - string - VARCHAR(25) - NOT NULL
concessionaria - string - VARCHAR(20) - NULL
motore - string - VARCHAR(20) - NOT NULL
cilindrata - string - VARCHAR(20) - NULL
lunghezza_auto - float - FLOAT(4,2) - NOT NULL
larghezza_auto - float - FLOAT(4,2) - NOT NULL
peso_auto - float - FLOAT(4,2) - NOT NULL
altezza_auto - float - FLOAT(4,2) - NOT NULL
dimensioni_ruote - float - FLOAT(4,2) - NOT NULL
dimensioni_bagagliaio - float - FLOAT(4,2) - NOT NULL
posti - integer - TINYINT - UNSIGNED - NOT NULL
garanzia - string - VARCHAR(10) - NULL
alimentazione - string - VARCHAR(20) - NOT NULL
classe_auto - string - VARCHAR(10) - NOT NULL
anno_fabbricazione - date - YEAR - NOT NULL
prezzo - float - DECIMAL(9,2) - NULL