altezza_auto - integer - TINYINT - NOTNULL
dimensioni_ruote - integer - TINYINT - NOTNULL
dimensioni_bagagliaio - integer - TINYINT - NOTNULL
posti - integer - TINYINT - NOTNULL
garanzia - string - VARCHAR(10) - NULL
